{
    "name": "publisher/publisher",
    "description": "Library to publish multiple entries (all at once).",
    "keywords": ["publisher", "publish", "post", "oauth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jacqueline Lueken",
            "email": "jac_lueken@live.de"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "suggest": {
        "publisher/entry_facebook": "Adds entries for posting on Facebook.",
        "publisher/entry_twitter": "Adds entries for posting on Twitter.",
        "publisher/entry_xing": "Adds entries for posting on XING.",
        "publisher/mode_recommendation": "Adds mode for publishing."
    },
    "autoload": {
        "psr-4": {
            "Publisher\\": "src/Publisher",
            "Unit\\": "tests/Unit",
            "Publisher\\Entry\\Service\\": "tests/Mock/Entry/Service",
            "Publisher\\Mode\\Foo\\": "tests/Mock/Mode/Foo",
            "Publisher\\Entry\\Service\\Mode\\Foo\\": "tests/Mock/ServiceFooEntity/Foo"
        }
    }
}